
cmd - f : yabai -m window --toggle zoom-fullscreen

cmd - h : yabai -m window --focus west
cmd - l : yabai -m window --focus east
cmd - k : yabai -m window --focus north
cmd - j : yabai -m window --focus south

shift + cmd - h : yabai -m window --warp west
shift + cmd - l : yabai -m window --warp east
shift + cmd - k : yabai -m window --warp north
shift + cmd - j : yabai -m window --warp south

cmd - 1 : yabai -m space --focus 1
cmd - 2 : yabai -m space --focus 2
cmd - 3 : yabai -m space --focus 3
cmd - 4 : yabai -m space --focus 4

shift + cmd - 1 : yabai -m window --space 1; yabai -m space --focus 1
shift + cmd - 2 : yabai -m window --space 2; yabai -m space --focus 2
shift + cmd - 3 : yabai -m window --space 3; yabai -m space --focus 3
shift + cmd - 4 : yabai -m window --space 4; yabai -m space --focus 4

cmd - i: yabai -m display --focus next
cmd - o: yabai -m display --focus prev

shift + cmd - i: yabai -m window --display next; yabai -m display --focus next
shift + cmd - o: yabai -m window --display prev; yabai -m display --focus prev

cmd - b : open -a Firefox\ Developer\ Edition 'https://google.com'
cmd - return : open -a iTerm ~

